# WalletConnect Project ID
# Get yours at https://cloud.walletconnect.com/
VITE_WALLETCONNECT_PROJECT_ID=YOUR_WALLETCONNECT_PROJECT_ID

# Token service configuration
# Set VITE_TOKEN_MODE=onchain to enable OnchainTokenService stubs
# Options: local | onchain
VITE_TOKEN_MODE=local

# Polygon network configuration
VITE_POLYGON_RPC_URL=https://polygon-rpc.com
VITE_CHAIN_ID=137

# Smart contract addresses (update when deployed to Polygon)
# These are placeholder addresses - replace with actual deployed contract addresses
VITE_TOKEN_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
VITE_STAKING_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
VITE_GOVERNOR_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
